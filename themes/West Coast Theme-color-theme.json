{
  "name": "West Coast Theme",
  "type": "dark",
  "colors": {
    "input.background": "#4a3b28",
    "dropdown.background": "#4a3b28",
    "editor.background": "#1a140c",
    "editor.foreground": "#d0a478",
    "focusBorder": "#9e6f42",
    "list.highlightForeground": "#dfab7a",
    "list.activeSelectionBackground": "#734a1e",
    "list.hoverBackground": "#734a1e66",
    "quickInputList.focusBackground": "#734a1eAA",
    "list.inactiveSelectionBackground": "#5c4838",
    "pickerGroup.foreground": "#dfab7a",
    "pickerGroup.border": "#dfab7a",
    "inputOption.activeBorder": "#9e6f42",
    "selection.background": "#7a5836aa",
    "editor.selectionBackground": "#7a5836aa",
    "minimap.selectionHighlight": "#7a5836aa",
    "editorWidget.background": "#0f120e",
    "editorHoverWidget.background": "#1a1510",
    "editorGroupHeader.tabsBackground": "#0f120e",
    "editorLineNumber.activeForeground": "#a8a8a8",
    "tab.inactiveBackground": "#0f120e",
    "tab.lastPinnedBorder": "#4a3b28",
    "titleBar.activeBackground": "#3d301f",
    "statusBar.background": "#3d301f",
    "statusBar.debuggingBackground": "#3d301f",
    "statusBar.noFolderBackground": "#3d301f",
    "statusBarItem.remoteBackground": "#655137",
    "ports.iconRunningProcessForeground": "#2d8c2e",
    "activityBar.background": "#1a140c",
    "activityBar.foreground": "#d0a478",
    "sideBar.background": "#32230e",
    "menu.background": "#32230e",
    "menu.foreground": "#c8c8c8",
    "editor.lineHighlightBackground": "#564027",
    "editorCursor.foreground": "#d0a478",
    "editorWhitespace.foreground": "#9e6f42",
    "peekViewTitle.background": "#32230e",
    "peekView.border": "#564027",
    "peekViewResult.background": "#32230e",
    "peekViewEditor.background": "#1a1510",
    "peekViewEditor.matchHighlightBackground": "#7a5836aa",
    "button.background": "#655137",
    "inputValidation.infoBorder": "#1a5a9f",
    "inputValidation.infoBackground": "#29263e",
    "inputValidation.warningBackground": "#4a3b28",
    "inputValidation.errorBackground": "#580c0c",
    "inputValidation.errorBorder": "#942a1f",
    "badge.background": "#765534",
    "progressBar.background": "#765534"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#d8d0d6",
        "background": "#1f2229"
      }
    },
    {
      "name": "Comment color",
      "scope": [
        "comment",
        "markup.quote.markdown",
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#958a90cc"
      }
    },
    {
      "name": "Text Color",
      "scope": [
        "meta.template.expression.js",
        "constant.name.attribute.tag.jade",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.string.begin.markdown"
      ],
      "settings": {
        "foreground": "#d8d0d6"
      }
    },
    {
      "name": "Teal",
      "scope": [
        "variable",
        "support.variable",
        "entity.name.tag.yaml",
        "constant.character.entity.html",
        "source.css entity.name.tag.reference",
        "beginning.punctuation.definition.list.markdown",
        "source.css entity.other.attribute-name.parent-selector",
        "meta.structure.dictionary.json support.type.property-name"
      ],
      "settings": {
        "foreground": "#4ecfad"
      }
    },
    {
      "name": "Amber",
      "scope": [
        "markup.bold",
        "constant.numeric",
        "meta.group.regexp",
        "constant.other.php",
        "support.constant.ext.php",
        "constant.other.class.php",
        "support.constant.core.php",
        "fenced_code.block.language",
        "constant.other.caps.python",
        "entity.other.attribute-name",
        "support.type.exception.python",
        "source.css keyword.other.unit",
        "variable.other.object.property.js.jsx",
        "variable.other.object.js"
      ],
      "settings": {
        "foreground": "#d4a865"
      }
    },
    {
      "name": "Warm Yellow",
      "scope": [
        "markup.list",
        "text.xml string",
        "entity.name.type",
        "support.function",
        "entity.other.attribute-name",
        "meta.at-rule.extend",
        "entity.name.function",
        "entity.other.inherited-class",
        "entity.other.keyframe-offset.css",
        "text.html.markdown string.quoted",
        "meta.function-call.generic.python",
        "meta.at-rule.extend support.constant",
        "entity.other.attribute-name.class.jade",
        "source.css entity.other.attribute-name",
        "text.xml punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#e8d174"
      }
    },
    {
      "name": "Coral",
      "scope": [
        "markup.heading",
        "variable.language.this.js",
        "variable.language.special.self.python"
      ],
      "settings": {
        "foreground": "#ff6b7a"
      }
    },
    {
      "name": "Rose",
      "scope": [
        "punctuation.definition.interpolation",
        "punctuation.section.embedded.end.php",
        "punctuation.section.embedded.end.ruby",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.begin.ruby",
        "punctuation.definition.template-expression",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#ff5c8a"
      }
    },
    {
      "name": "Lavender",
      "scope": [
        "storage",
        "keyword",
        "meta.link",
        "meta.image",
        "markup.italic",
        "source.js support.type"
      ],
      "settings": {
        "foreground": "#a78bfa"
      }
    },
    {
      "name": "Sky Blue",
      "scope": ["string.regexp", "markup.changed"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Soft Red",
      "scope": [
        "constant",
        "support.class",
        "keyword.operator",
        "support.constant",
        "text.html.markdown string",
        "source.css support.function",
        "source.php support.function",
        "support.function.magic.python",
        "entity.other.attribute-name.id",
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#ff5757"
      }
    },
    {
      "name": "Sage Green",
      "scope": [
        "string",
        "text.html.php string",
        "markup.inline.raw",
        "markup.inserted",
        "punctuation.definition.string",
        "punctuation.definition.markdown",
        "text.html meta.embedded source.js string",
        "text.html.php punctuation.definition.string",
        "text.html meta.embedded source.js punctuation.definition.string",
        "text.html punctuation.definition.string",
        "text.html string"
      ],
      "settings": {
        "foreground": "#86d973"
      }
    },
    {
      "name": "Font Underline",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "underline"
      }
    }
  ],
  "semanticTokenColors": {
    "property.declaration:javascript": "#d8d0d6",
    "variable.defaultLibrary:javascript": "#d4a865"
  },
  "semanticHighlighting": true
}
